{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "dom",
            "es2018"
        ],
        "module": "commonjs",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "out",
        "rootDir": "src",
        "sourceMap": true,
        "strict": true,
        "target": "es2018",
        "skipLibCheck": true
    }
}
